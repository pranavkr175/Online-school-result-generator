<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>StudentReg</servlet-name>
  	<display-name>StudentReg</display-name>
  	<description></description>
  	<servlet-class>servlet.StudentReg</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddMarks</servlet-name>
  	<display-name>AddMarks</display-name>
  	<description></description>
  	<servlet-class>servlet.AddMarks</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateStudent</servlet-name>
  	<display-name>UpdateStudent</display-name>
  	<description></description>
  	<servlet-class>servlet.UpdateStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminLogin</servlet-name>
  	<display-name>AdminLogin</display-name>
  	<description></description>
  	<servlet-class>admin.servlet.AdminLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminLogout</servlet-name>
  	<display-name>AdminLogout</display-name>
  	<description></description>
  	<servlet-class>admin.servlet.AdminLogout</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>StudentReg</servlet-name>
  	<url-pattern>/StudentReg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddMarks</servlet-name>
  	<url-pattern>/AddMarks</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateStudent</servlet-name>
  	<url-pattern>/UpdateStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminLogin</servlet-name>
  	<url-pattern>/AdminLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminLogout</servlet-name>
  	<url-pattern>/AdminLogout</url-pattern>
  </servlet-mapping>
 
  
</web-app>
